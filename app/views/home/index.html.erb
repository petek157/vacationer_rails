<%= react_component("Logo", {image: "#{image_url "flatVacationerLogo.png"}"}) %>
<%= react_component("Slider") %>
<%= react_component("InfoSection") %>
<% @features.each do |f| %>
    <%= react_component("FeatureSection", {position: f.position.to_s, 
    title: f.title,
    text: f.text,
    link: f.link,
    align: f.align,
    x: f.x,
    y: f.y,
    theme: f.theme,
    image: url_for(f.image.variant(resize_to_fit: [nil, 600])),
    }) %>
<% end %>

