<%= react_component("Logo", {image: "#{image_url "vacationerLogo.png"}"}) %>
<%= react_component("Slider") %>

<%= react_component("BusinessCategories", {theme: "fall", showSub: true}) %>
<% if @ad %>
    <%= react_component("FeatureAd", {large: "#{url_for @ad.large}", medium: "#{url_for @ad.medium}", small: "#{url_for @ad.small}", link: "#{@ad.url}"})%>
<% end %>
<% @features.each do |f| %>
    <%= react_component("FeatureSection", {position: f.position.to_s, 
    title: f.title,
    text: f.text,
    link: f.link,
    align: f.align,
    x: f.x,
    y: f.y,
    theme: f.theme,
    image: url_for(f.image.variant(resize_to_fill: [640, 250])),
    }) %>
<% end %>
<%= react_component("InfoSection", {theme: "winter"}) %>
<footer>

</footer>